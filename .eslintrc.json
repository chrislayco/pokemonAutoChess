{
  "extends": ["google"],
  "env": {
    "node": true,
    "es2017": true
  },
  "rules": {
    "camelcase": ["error", {
      "properties": "always"
    }],
    "comma-dangle": ["error", "never"],
    "guard-for-in": "off",
    "linebreak-style": ["error", "windows"],
    // "max-len": ["warn", {
    //   "code": 160,
    //   "tabWidth": 2,
    //   "ignoreUrls": true
    // }],
    "max-len": "off",
    "require-jsdoc": "off"
  }
}
